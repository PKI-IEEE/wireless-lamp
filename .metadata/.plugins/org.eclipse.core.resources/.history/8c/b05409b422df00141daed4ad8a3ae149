#include "wifi.h"
void setWifi() {
	char setMode[] = "wifi.setmode(wifi.STATION)";
	char staConfig[] = "wifi.sta.config(\"YOUR SSID\",\"NETWORK PASSWORD\")"; // enter your network SSID and password. Notice the escaped quotes. SSID and password must be in quotes
	char staConnect[] = "wifi.sta.connect()";
	char CRLF[2] = {0x0D, 0x0A};;


	UARTSend(setMode,sizeof(setMode)-1);
	UARTSend(CRLF,2);
	delay_us(100);
	UARTSend(staConfig,sizeof(staConfig)-1);
	UARTSend(CRLF,2);
	delay_us(100);
	UARTSend(staConnect,sizeof(staConnect)-1);
	UARTSend(CRLF,2);
	delay_us(100);
}

